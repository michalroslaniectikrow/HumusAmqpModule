{
    "name": "prolic/humus-amqp-module",
    "type": "library",
    "description": "AMQP module for Laminas to integrate RabbitMQ",
    "keywords": [
        "laminas",
        "rabbit",
        "rabbitmq",
        "queue",
        "amqp",
        "humus"
    ],
    "homepage": "https://github.com/prolic/HumusAmqpModule",
    "license": "MIT",
    "authors": [
        {
            "name": "Sascha-Oliver Prolic",
            "email": "saschaprolic@googlemail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "prolic/humus-amqp": "^2.1",
        "psr/container": "^1.0",
        "sandrokeil/interop-config": "^2.2",
        "symfony/console": "^5.4",
        "laminas/laminas-modulemanager": "^2.12",
        "laminas/laminas-mvc": "^3.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.11",
        "phpunit/phpunit": "^9.5",
        "php-coveralls/php-coveralls": "^2.5",
        "prooph/php-cs-fixer-config": "^0.5.0"
    },
    "autoload": {
        "psr-4": {
            "HumusAmqpModule\\": "src/",
            "HumusAmqpModuleTest\\": "tests/"
        }
    },
    "bin": [
        "bin/humus-amqp-module"
    ]
}
